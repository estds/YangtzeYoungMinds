---
search_inclusion: false
---
[
  {% for post in site.posts %}
  {
  "title" : "{{ post.title | escape }}",
  "abstract" : "{{post.abstract | strip_html | strip_newlines | escape }}",
  "url": "{{ post.url | prepend: site.baseurl }}"
  },
  {% endfor %}
  {% for page in site.pages %}
  {
  "title" : "{{ page.title | escape }}",
  "abstract" : {% if page.search_inclusion == no %}"Not included"{% else %}"{{page.abstract | strip_html | strip_newlines | escape }}"{% endif %},
  "url" : "{{ page.url | prepend: site.baseurl }}"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]